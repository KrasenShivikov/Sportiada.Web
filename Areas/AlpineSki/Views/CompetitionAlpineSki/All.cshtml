@model IEnumerable<CompetitionAlpineSkiModel>

@{
    ViewData["Title"] = "Състезания Ски Алпийски Дисциплини";
}

<h2>Състезания Ски Алпийски Дисциплини</h2>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Дата и час</th>
            <th>Дисциплина</th>
            <th>Място</th>
            <th>Сезон</th>
            <th>Турнир</th>
            <th>Тип</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var competition in Model)
        {
            <tr>
                <td>@competition.DateTime</td>
                <td><a href="/alpineSki/competition/@competition.Id/ПЪРВИ МАНШ">@competition.Discipline.Name</a></td>
                <td>@competition.PlaceAlpineSki.Name</td>
                <td>@competition.Season.Name</td>
                <td>@competition.Tournament.Name</td>
                <td>@competition.CompetitionType.Name</td>
            </tr>
        }
    </tbody>
</table>



