@model FootballPlayerOutDeleteModel

@{
    ViewData["Title"] = "DeletePlayerOut";
}

<h2>Наистина ли желаете да изтриете</h2>

<table class="table table-bordered">
    <tr>
        <td><img src="@Url.Content(Model.PlayerCountry)" height="20" width="30" /> @Model.PlayerName <img src="@Url.Content(Model.OutIcon)" height="20" width="20" /></td>
    </tr>
</table>

<a href="/admin/deletePlayerOut" class="btn btn-success">Да</a><a href="/admin/footballGame/@Model.GameId" class="btn btn-danger">Не</a>

